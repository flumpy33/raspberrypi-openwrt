-#ifdef DEBUG
-static inline void dump_bytes(char *name, uint8_t *bytes, int len)
-{
-	int i;
-	DWC_PRINTF("%s: ", name);
-	for (i=0; i<len; i++) {
-		DWC_PRINTF("%02x ", bytes[i]);
-	}
-	DWC_PRINTF("\n");
-}
-#else
-#define dump_bytes(x...)
-#endif
-
-/* Display a block */
-void show_block(const u8 *blk, const char *prefix, const char *suffix, int a)
-{
-#ifdef DEBUG_CRYPTO
-	int i, blksize = 16;
-
-	DWC_DEBUG("%s", prefix);
-
-	if (suffix == NULL) {
-		suffix = "\n";
-		blksize = a;
-	}
-
-	for (i = 0; i < blksize; i++)
-		DWC_PRINT("%02x%s", *blk++, ((i & 3) == 3) ? "  " : " ");
-	DWC_PRINT(suffix);
-
-#endif
-}
